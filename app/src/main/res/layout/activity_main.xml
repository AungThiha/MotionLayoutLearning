<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:orientation="vertical">

    <Button
        android:id="@+id/simpleViewLeftRightButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="simpleViewLeftRightButton" />

    <Button
        android:id="@+id/imageFilterFaceButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="imageFilterFaceButton" />

    <Button
        android:id="@+id/imageFilterSaturationButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="imageFilterSaturationButton" />

    <Button
        android:id="@+id/positionKeyFrameButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="positionKeyFrameButton" />

    <Button
        android:id="@+id/attributeKeyFrameButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="attributeKeyFrameButton" />

    <Button
        android:id="@+id/airplaneButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="airplaneButton" />

    <Button
        android:id="@+id/rotatingTextCoordinatorLayoutButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="rotatingTextCoordinatorLayoutButton" />

</LinearLayout>